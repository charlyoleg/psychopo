{
  "name": "psychopo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -fr dist",
    "partial_clean": "rm -fr dist/css dist/podcast dist/*.html dist/*.png dist/*.svg dist/drawings dist/fables_de_jean_de_la_fontaine dist/mentions_legales",
    "cp_audio": "mkdir -p dist/audio && rsync -r --update --progress audio dist/",
    "build_css": "sass sass/psychopo_style.scss dist/css/psychopo_style.css",
    "dev_css": "sass --watch sass/psychopo_style.scss dist/css/psychopo_style.css",
    "create_missing_json": "node scr/create_json.js",
    "complete_json": "node scr/complete_json.js",
    "enhance_audio_titles": "node scr/enhance_audio_titles.js",
    "testing_audio_list": "node scr/test_audio_list.js",
    "check_empty_json": "node scr/check_empty_string.js",
    "update_json": "run-s create_missing_json complete_json enhance_audio_titles testing_audio_list check_empty_json",
    "build_eleventy": "eleventy",
    "build_all": "run-p cp_audio build_css build_eleventy",
    "serve": "eleventy --serve",
    "dev": "eleventy --watch",
    "view": "sensible-browser http://localhost:8080",
    "live-serve": "live-server dist/",
    "publish": "rsync -r --update --progress dist/ userabc@myserver.ovh:www/",
    "dist_size": "du -sh dist",
    "dist_mp3_list": "find dist -type f -iname '*.mp3' -exec md5sum {} + | sort -k 1 > dist_mp3.txt",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "^0.10.0",
    "json5": "^2.1.1",
    "live-server": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.25.0"
  }
}
